#! /usr/bin/vvp
:ivl_version "12.0 (stable)" "(v12_0-dirty)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/ivl/system.vpi";
:vpi_module "/usr/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/ivl/va_math.vpi";
S_0x5d3b0c6246c0 .scope module, "d_flipflop_testbench" "d_flipflop_testbench" 2 3;
 .timescale -9 -12;
v0x5d3b0c64bb30_0 .var "D", 0 0;
v0x5d3b0c64bbf0_0 .net "Q", 0 0, v0x5d3b0c64b890_0;  1 drivers
v0x5d3b0c64bcc0_0 .var "clk", 0 0;
v0x5d3b0c64bdc0_0 .var "resetn", 0 0;
S_0x5d3b0c6379c0 .scope module, "uut" "D_FlipFLop" 2 8, 3 1 0, S_0x5d3b0c6246c0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "resetn";
    .port_info 2 /INPUT 1 "D";
    .port_info 3 /OUTPUT 1 "Q";
v0x5d3b0c637c10_0 .net "D", 0 0, v0x5d3b0c64bb30_0;  1 drivers
v0x5d3b0c64b890_0 .var "Q", 0 0;
v0x5d3b0c64b950_0 .net "clk", 0 0, v0x5d3b0c64bcc0_0;  1 drivers
v0x5d3b0c64b9f0_0 .net "resetn", 0 0, v0x5d3b0c64bdc0_0;  1 drivers
E_0x5d3b0c636720/0 .event negedge, v0x5d3b0c64b9f0_0;
E_0x5d3b0c636720/1 .event posedge, v0x5d3b0c64b950_0;
E_0x5d3b0c636720 .event/or E_0x5d3b0c636720/0, E_0x5d3b0c636720/1;
    .scope S_0x5d3b0c6379c0;
T_0 ;
    %wait E_0x5d3b0c636720;
    %load/vec4 v0x5d3b0c64b9f0_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5d3b0c64b890_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5d3b0c637c10_0;
    %assign/vec4 v0x5d3b0c64b890_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5d3b0c6246c0;
T_1 ;
    %delay 5000, 0;
    %load/vec4 v0x5d3b0c64bcc0_0;
    %inv;
    %store/vec4 v0x5d3b0c64bcc0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5d3b0c6246c0;
T_2 ;
    %vpi_call 2 19 "$dumpfile", "dff_test.vcd" {0 0 0};
    %vpi_call 2 20 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5d3b0c6246c0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d3b0c64bcc0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d3b0c64bdc0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d3b0c64bb30_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5d3b0c64bdc0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5d3b0c64bb30_0, 0, 1;
    %delay 10000, 0;
    %vpi_call 2 31 "$display", "D = %b, Q = %b (Expected: 1)", v0x5d3b0c64bb30_0, v0x5d3b0c64bbf0_0 {0 0 0};
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d3b0c64bb30_0, 0, 1;
    %delay 10000, 0;
    %vpi_call 2 35 "$display", "D = %b, Q = %b (Expected: 0)", v0x5d3b0c64bb30_0, v0x5d3b0c64bbf0_0 {0 0 0};
    %delay 5000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d3b0c64bdc0_0, 0, 1;
    %delay 5000, 0;
    %vpi_call 2 39 "$display", "After reset: Q = %b (Expected: 0)", v0x5d3b0c64bbf0_0 {0 0 0};
    %delay 10000, 0;
    %vpi_call 2 42 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "FlipFlop/D_FlipFlop_tb.v";
    "FlipFlop/D_FlipFlop.v";
